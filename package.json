{
  "name": "webapp",
  "version": "2.1.1",
  "scripts": {
    "analyze": "ng build --configuration production --stats-json && webpack-bundle-analyzer dist/webapp/stats.json",
    "build": "ng build",
    "build:prod": "NODE_ENV=production ng build --configuration production",
    "format": "prettier --write \"src/**/*.{js,ts,json,md,css,scss,html}\"",
    "lint": "eslint --color --fix -c .eslintrc.js --ext .ts ./src/app",
    "ng": "ng",
    "ng-cdk": "ng update --force @angular/cdk",
    "ng-cli": "ng update --force @angular/cli",
    "ng-core": "ng update --force @angular/core",
    "ng-material": "ng update --force @angular/material",
    "prepare": "husky install",
    "start": "ng serve",
    "start:auth": "ng serve --port=3000 --configuration=localAuth",
    "test": "jest --watch --maxWorkers=1",
    "test:ci": "jest --runInBand",
    "add-license-header": "node ./scripts/add-license-header.js --src=src"
  },
  "private": true,
  "dependencies": {
    "@angular-material-components/datetime-picker": "7.0.1",
    "@angular/animations": "13.3.6",
    "@angular/cdk": "13.3.6",
    "@angular/common": "13.3.6",
    "@angular/compiler": "13.3.6",
    "@angular/core": "13.3.6",
    "@angular/forms": "13.3.6",
    "@angular/material": "13.3.6",
    "@angular/platform-browser": "13.3.6",
    "@angular/platform-browser-dynamic": "13.3.6",
    "@angular/router": "13.3.6",
    "@types/lodash-es": "4.17.6",
    "autoprefixer": "10.4.7",
    "d3": "7.4.4",
    "keycloak-angular": "9.1.0",
    "keycloak-js": "18.0.0",
    "lodash-es": "4.17.21",
    "luxon": "2.3.2",
    "postcss": "8.4.13",
    "rxjs": "7.5.5",
    "tailwindcss": "3.0.24",
    "tslib": "2.4.0",
    "uuid": "8.3.2",
    "webpack": "5.72.0",
    "webpack-bundle-analyzer": "4.5.0",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "13.1.0",
    "@angular-devkit/build-angular": "13.3.5",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "^13.2.1",
    "@angular/cli": "13.3.5",
    "@angular/compiler-cli": "13.3.6",
    "@angular/language-service": "13.3.6",
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@testing-library/angular": "^11.0.4",
    "@testing-library/jest-dom": "^5.16.4",
    "@types/d3": "7.1.0",
    "@types/jest": "^27.5.0",
    "@types/luxon": "2.3.2",
    "@types/node": "17.0.31",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.2.9",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "husky": "7.0.4",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-extended": "^2.0.0",
    "jest-preset-angular": "^12.0.0",
    "lint-staged": "^12.4.1",
    "msw": "^0.39.2",
    "postcss-import": "14.1.0",
    "postcss-loader": "6.2.1",
    "postcss-scss": "4.0.4",
    "prettier": "2.6.2",
    "stylelint": "14.8.2",
    "stylelint-config-standard-scss": "3.0.0",
    "ts-node": "10.7.0",
    "typescript": "4.6.4"
  },
  "msw": {
    "workerDirectory": "src"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "<rootDir>/src/tsconfig.spec.json"
      }
    },
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/tests/setupJest.ts"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!internmap|delaunator|robust-predicates|d3|keycloak-angular|@testing-library|@angular|lodash-es)"
    ],
    "testEnvironment": "jest-environment-jsdom",
    "moduleNameMapper": {
      "@core/(.+)": "<rootDir>src/app/modules/core/$1",
      "@shared": "<rootDir>src/app/modules/shared/index",
      "@shared/(.+)": "<rootDir>src/app/modules/shared/$1",
      "@layout/(.+)": "<rootDir>src/app/modules/core/layout/$1",
      "@page/(.+)": "<rootDir>src/app/modules/page//$1",
      "@env/(.+)": "<rootDir>src/environments/environment/$1",
      "@tests/(.+)": "<rootDir>/src/tests/$1",
      "src/(.+)": "<rootDir>src/$1"
    }
  }
}
